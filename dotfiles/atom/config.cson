{{@@ header('# ') @@}}
"*":
  core:
    audioBeep: false
    disabledPackages: [
      "autocomplete-snippets"
      "autoflow"
      "background-tips"
      "git-log"
      "open-on-github"
      "github"
      "language-bison-flex"
    ]
    telemetryConsent: "limited"
  editor:
    fontSize: 18
    softTabs: false
    softWrap: true
    tabLength: 4
  "exception-reporting":
    userId: "{{@@ atom_user_id @@}}"
  linter:
    lintOnChange: false
  "linter-ui-default": {}
  "one-dark-ui": {}
  "spell-check":
    locales: [
      "en-US"
      "it-IT"
    ]
  tabs:
    addNewTabsAtEnd: true
    displayMruTabList: false
    enableMruTabSwitching: false
    enableVcsColoring: true
  "tree-view":
    hideVcsIgnoredFiles: false
  welcome:
    showOnStartup: false
